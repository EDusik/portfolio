{
	"printWidth": 240,
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"quoteProps": "preserve",
	"jsxSingleQuote": false,
	"trailingComma": "none",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"arrowParens": "avoid",
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "preserve",
	"htmlWhitespaceSensitivity": "strict",
	"endOfLine": "auto",
	"embeddedLanguageFormatting": "auto",
	"tabWidth": 2
}
